<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>wp02</groupId>
  <artifactId>WP02</artifactId>
  <version>1.0.0</version>
  <packaging>war</packaging>
  <name>WP02</name>
  <description/>
  <url>http://www.emc.com/xcp</url>
  <properties>
  	<creationDate>2015-02-13T11:19:45.790+0000</creationDate>  	
  </properties>

	  
  <build>
    <finalName>wp02-WP02-${version}</finalName>
    <plugins>
      <plugin>
         <groupId>com.emc.xcp.builder</groupId>
         <artifactId>xcp-import-project</artifactId>
         <version>1.0.4</version>
      </plugin>
      <plugin>
         <groupId>com.emc.xcp.builder</groupId>
         <artifactId>xcp-validate</artifactId>
         <version>1.0.9</version>
         <executions>
            <execution>
               <id>xcp-validate</id>
               <goals>
                  <goal>run</goal>
               </goals>
            </execution>
         </executions>
         <configuration>
         	<dir>${basedir}\Artifacts</dir>
         </configuration>
      </plugin>
      <plugin>
		 <groupId>org.codehaus.mojo</groupId>
		 <artifactId>build-helper-maven-plugin</artifactId>
		 <version>1.7</version>
		 <executions>
            <execution>
                 <id>add-source</id>
                    <phase>generate-sources</phase>
                    <goals>
	                    <goal>add-source</goal>
                    </goals>
                    <configuration>
	                    <sources>
	                        <source>gen/main/java</source>
	                    </sources>
                    </configuration>
            </execution>
			<execution>
	            <id>add-resource</id>
	            <phase>generate-resources</phase>
	            <goals>
	                <goal>add-resource</goal>
	            </goals>
	            <configuration>
	                <resources>
						<resource>
			              	<directory>gen/main/resources</directory>
						</resource>
					</resources>
           		</configuration>
   			</execution>                                
		</executions>
	  </plugin>
      <plugin>
         <groupId>com.emc.xcp.builder</groupId>
         <artifactId>xcp-generate-sources</artifactId>
         <version>1.0.9</version>
         <executions>
            <execution>
               <id>xcp-generate-sources</id>
               <phase>generate-sources</phase>
               <goals>
                  <goal>run</goal>
               </goals>
            </execution>
         </executions>
         <configuration>
         	<dir>${basedir}\Artifacts</dir>
         </configuration>
      </plugin>
	  <plugin>
          <groupId>org.apache.maven.plugins</groupId>
		<artifactId>maven-antrun-plugin</artifactId>
		<executions>
			<execution>
				<id>copy-resources-for-m2e-webby</id>
				<phase>prepare-package</phase>
				<goals>
					<goal>run</goal>
				</goals>
				<configuration/>
			</execution>
		</executions>
	  </plugin>
	  <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-war-plugin</artifactId>
          <version>2.1.1</version>
          <configuration>
              <archive>
                  <manifestFile>META-INF/MANIFEST.MF</manifestFile>
              </archive>
              <webResources>
		  		<resource>
		  			<directory>Artifacts</directory>
		  			<includes>
		  				<include>**/*</include>
		  			</includes>
		  			<targetPath>Artifacts</targetPath>
		  		</resource>
		  		<resource>
		  			<directory>content</directory>
		  			<includes>
		  				<include>**/*</include>
		  			</includes>
		  			<targetPath>content</targetPath>
		  		</resource>
				<resource>
				    <directory>lib</directory>
				    <includes>
				       <include>**/*.jar</include>
				    </includes>
				    <targetPath>WEB-INF/lib</targetPath>
				</resource>
				<resource>
				    <directory>gen/main/webapp</directory>
				    <includes>
				       <include>**/*</include>
				    </includes>
				</resource>
		  		<resource>
		  			<directory>.</directory>
		  			<includes>
		  				<include>.index</include>
		  			</includes>
		  		</resource>
              </webResources>
			 <escapeString>\</escapeString>
          </configuration>
      </plugin>

      <plugin>
         <groupId>com.emc.xcp.builder</groupId>
         <artifactId>xcp-dar</artifactId>
         <version>1.0.11</version>
         <executions>
            <execution>
               <id>xcp-dar</id>
               <phase>prepare-package</phase>
               <goals>
                  <goal>run</goal>
               </goals>
            </execution>
         </executions>
         <configuration>
         	<dir>${basedir}\Artifacts</dir>
         	<executionEnvironment>
         		<groupId>com.emc.executionenvironment</groupId>
         		<artifactId>xcp</artifactId>
         		<version>2.1.5</version>
         	</executionEnvironment>
         <Manifest>
            <Bundle-ClassPath>.,lib/MyDocumentumUtils.jar</Bundle-ClassPath>
          </Manifest>
        </configuration>
      </plugin>

      <plugin>
         <groupId>com.emc.xcp.builder</groupId>
         <artifactId>xcp-deploy</artifactId>
         <version>1.0.11</version>
         <executions>
            <execution>
               <id>xcp-deploy</id>
               <goals>
                  <goal>run</goal>
               </goals>
            </execution>
         </executions>
         <configuration>
         	<envId>${envId}</envId>
         	<dataPolicy>${dataPolicy}</dataPolicy>
         	<deploymentMethod>${deploymentMethod}</deploymentMethod>
			<parameters>
  				<configFileName>${configFileName}</configFileName> 
				<com.emc.executionenvironment.xcp.xPlore.xploreAsyncIndexing>${com.emc.executionenvironment.xcp.xPlore.xploreAsyncIndexing}</com.emc.executionenvironment.xcp.xPlore.xploreAsyncIndexing>
  			</parameters>
         </configuration>
      </plugin>

      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.5</version>
        <executions>
          <execution>
            <id>default-deploy</id>
            <phase>deploy</phase>
            <goals>
              <goal>deploy</goal>
            </goals>
            <configuration>
            	<skip>true</skip>
            </configuration>
          </execution>
        </executions>
      </plugin>

	  <plugin>
		<groupId>org.apache.maven.plugins</groupId>
		<artifactId>maven-dependency-plugin</artifactId>
		<version>2.3.emcpatch</version>
		<configuration>
			<forcedPlugins>
				<param>org.apache.maven.plugins:maven-surefire-plugin:2.7.1</param>
				<param>org.apache.maven.surefire:surefire-junit3:2.10</param>
				<param>org.apache.maven.surefire:surefire-providers:2.10:pom</param>
			</forcedPlugins>
		</configuration>
		<executions>
		    <execution>
				<id>patched-maven-dependency-plug-in</id>
				<goals>
					<goal>go-offline</goal>
				</goals>
			</execution>
		</executions>
	   </plugin>
	   
	   <plugin>
	        <groupId>org.apache.maven.plugins</groupId>
	        <artifactId>maven-compiler-plugin</artifactId>
	        <version>2.3.2</version>
	        <configuration>
	           <source>1.6</source>
	           <target>1.6</target>
	        </configuration>
		</plugin>
		

    </plugins>
  </build>
  <repositories>
  	<repository>
  		<id>Builder</id>
  		<url>http://localhost:2910/maven</url>
  	</repository>
	
	
	
	
	
 	
	
	
	
	
	  	
	
	
	
	
	
	
	
	
	
	
	
	
 	
 	
 	
	
	
	
	
	
	
 	
 	
	
	
 	
	
	
	

			
